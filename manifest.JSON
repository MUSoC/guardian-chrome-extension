{
"manifest_version": 2,
    "name": "Guardian",
    "description": "Blocks offensive words",
    "version": "1.0",
         "page_action": {
          "default_icon": "icon.png"  
        },
        "icons": {
           "16": "16.png",
           "24": "24.png",
           "32": "32.png",
           "48": "48.png",
          "128": "128.png" },
          "options_page": "options.html",
          "permissions": [
          "storage"
        ],
          "options_page": "options.html",
          "content_security_policy":"script-src 'self' https://apis.google.com; object-src 'self'",
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ]
}